def info (c,name,high,age,lifeexpectancy):
  if c==0:
    print (f"Дереву - {name} сегодня {age} лет. Его рост в эти первые дни = {high} метров. Впереди у него долгая и счастливая жизнь. Предположительно, он будет расти {lifeexpectancy} лет\n")
  print (f"{name} сегодня - {age} лет. Его рост составляет - {high} метров\n")
